            "rawQuery": true,
            "rawSql": "SELECT \n  apt.start at time zone 'UTC' as start_budapest,\n  regexp_replace(seat.full_name,'\\S*$','') as name,\n  seat.appointment_id,\n  regexp_replace(apt.email,'^.{0,8}','***') as apt_email\nFROM appointments_seat as seat\nINNER JOIN (\n  SELECT \n    appt.uuid as uuid,\n    appt.email as email,\n    slot.start as start\n  FROM appointments_appointment AS appt\n  INNER JOIN appointments_timeslot AS slot\n    ON appt.time_slot_id=slot.uuid\n  WHERE appt.is_registration_completed = 'true'  \n) AS apt\n  ON apt.uuid=seat.appointment_id\nWHERE\n  DATE(apt.start) = CURRENT_DATE\n  AND\n  seat.uuid NOT IN (\n    SELECT seat_id FROM samples_sample\n  )",
